what <- switch(
substring(match, 2, 2),
"H" = tempo$hour_of(x),
"M" = tempo$minute_of(x),
"S" = tempo$second_of(x),
"h" = tempo$in_hours(x),
"m" = tempo$in_minutes(x),
"s" = tempo$in_seconds(x)
)
formatC(what,
width = as.numeric(substring(match, 3, 3)),
flag = "0")
}
